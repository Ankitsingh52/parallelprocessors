#include<bits/stdc++.h>
using namespace std;
int main()
{
    int i,j,n,t;
    cin>>n;
    int a[n];
    int sum=0;
    for(i=0;i<n;i++)
    {
        cin>>a[i];
        sum+=a[i];
    }

    if(sum%9==0)
    {
        cout<<"9\n";
    }
    else
    {
        cout<<sum%9<<"\n";
    }

    return 0;
}
