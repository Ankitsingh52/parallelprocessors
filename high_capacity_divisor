#include<bits/stdc++.h>
using namespace std;
int main()
{
    fast();
    string s;
    ll x;
    cin>>s>>x;
    ll res=0;
    for(int i=0;i<s.size();i++){
        res*=10;
        res+=s[i]-'0';
        res%=x;

    }
    if(res==0){
        cout<<"YES\n";
    }
    else{
        cout<<"NO\n";
    }

        
    
    return 0;
